{
  "name": "inventa-react",
  "version": "1.0.0",
  "description": "Modern React TypeScript business management application",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "electron": "electron .",
